{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "require-jsdoc": 0,
        "max-len": [
            "error",
            {
                "code": 150,
                "tabWidth": 2
            }
        ]
    }
}